{
  "name": "seurantakiinnostus-demo",
  "version": "1.0.0",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "format-check": "prettier '{*,{src}/**/*}.{ts,tsx,js,jsx,html,json,css,yml}' --check",
    "lint-check": "tslint --project tsconfig.json --config tslint.json '{src}/**/*.{ts,tsx}'",
    "check": "npm run format-check && npm run type-check && npm run lint-check",
    "build": "webpack --progress --colors --mode=production",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --mode=development",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^10.17.13",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-define": "^2.0.0",
    "css-loader": "^1.0.1",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.2",
    "ts-lint": "^4.5.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "axios": "^0.21.1",
    "json-loader": "^0.5.7",
    "leaflet": "^1.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-leaflet": "^2.6.1",
    "styled-components": "^4.4.1"
  }
}
